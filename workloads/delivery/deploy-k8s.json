{
    "name": "delivery",
    "region": "us-east-1",
    "allowedPorts": [
        8083,
        8086,
        7777,
        7778,
        8089,
        5557,
        5558,
        8080,
        3000,
        7878
    ],
    "files": [
        {
            "fileId": "delivery_load_test",
            "path": "\/home\/ec2-user\/locust_file\/locustfile.py"
        }
    ],
    "nodeMapping": [
        {
            "task": "goddd",
            "id": 2
        },
        {
            "task": "pathfinder",
            "id": 3
        },
        {
            "task": "dddelivery-angularjs",
            "id": 1
        },
        {
            "task": "benchmark-agent",
            "id": 2
        },
        {
            "task": "monitor",
            "id": 2
        },
        {
            "task": "snap",
            "id": 2
        },
        {
            "task": "locust-master",
            "id": 1
        },
        {
            "count": 1,
            "task": "locust-slave",
            "id": 1
        }
    ],
    "clusterDefinition": {
        "nodes": [
            {
                "instanceType": "t2.large",
                "id": 1
            },
            {
                "instanceType": "t2.large",
                "id": 2
            },
            {
                "instanceType": "t2.large",
                "id": 3
            },
            {
                "instanceType": "t2.large",
                "id": 4
            }
        ]
    },
    "kubernetes": {
        "taskDefinitions": [
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "benchmark-agent"
                        },
                        "name": "benchmark-agent"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "benchmark-agent"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "benchmark-agent"
                                }
                            },
                            "spec": {
                                "containers": [
                                    {
                                        "image": "hyperpilot\/benchmark-agent",
                                        "name": "benchmark-agent",
                                        "ports": [
                                            {
                                                "containerPort": 6001,
                                                "hostPort": 6001,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "125.0m",
                                                "memory": "150Mi"
                                            }
                                        },
                                        "volumeMounts": [
                                            {
                                                "mountPath": "\/var\/run\/docker.sock",
                                                "name": "docker-sock"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "benchmark-agent"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "snap",
                            "version": "latest"
                        },
                        "name": "snap",
                        "namespace": "default"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "snap",
                                "version": "latest"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "snap",
                                    "version": "latest"
                                }
                            },
                            "spec": {
                                "containers": [
                                    {
                                        "image": "wen777\/snap:alpine",
                                        "name": "snap",
                                        "resources": {
                                            "limits": {
                                                "cpu": "125.0m",
                                                "memory": "200Mi"
                                            }
                                        },
                                        "volumeMounts": [
                                            {
                                                "mountPath": "\/rootfs",
                                                "name": "root"
                                            },
                                            {
                                                "mountPath": "\/var\/run",
                                                "name": "var-run"
                                            },
                                            {
                                                "mountPath": "\/var\/log",
                                                "name": "var-log"
                                            },
                                            {
                                                "mountPath": "\/sys",
                                                "name": "sys"
                                            },
                                            {
                                                "mountPath": "\/sys\/fs\/cgroup",
                                                "name": "cgroup"
                                            },
                                            {
                                                "mountPath": "\/var\/lib\/docker",
                                                "name": "var-lib-docker"
                                            }
                                        ]
                                    }
                                ],
                                "volumes": [
                                    {
                                        "hostPath": {
                                            "path": "\/"
                                        },
                                        "name": "root"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/cgroup"
                                        },
                                        "name": "cgroup"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/sys"
                                        },
                                        "name": "sys"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/var\/lib\/docker\/"
                                        },
                                        "name": "var-lib-docker"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/var\/log"
                                        },
                                        "name": "var-log"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/var\/run"
                                        },
                                        "name": "var-run"
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "snap"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "influxsrv"
                        },
                        "name": "influxsrv",
                        "namespace": "default"
                    },
                    "spec": {
                        "replicas": 1,
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "influxsrv"
                                }
                            },
                            "spec": {
                                "hostname": "influxsrv",
                                "containers": [
                                    {
                                        "env": [
                                            {
                                                "name": "ADMIN_USER",
                                                "value": "root"
                                            },
                                            {
                                                "name": "PRE_CREATE_DB",
                                                "value": "snap;goddd;pathfinder"
                                            },
                                            {
                                                "name": "INFLUXDB_INIT_PWD",
                                                "value": "hyperpilot"
                                            }
                                        ],
                                        "image": "tutum\/influxdb",
                                        "name": "influxsrv",
                                        "ports": [
                                            {
                                                "containerPort": 8083,
                                                "hostPort": 8083,
                                                "protocol": "TCP"
                                            },
                                            {
                                                "containerPort": 8086,
                                                "hostPort": 8086,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "500m",
                                                "memory": "300Mi"
                                            }
                                        }
                                    },
                                    {
                                        "env": [
                                            {
                                                "name": "INFLUXDB_NAME",
                                                "value": "snap"
                                            },
                                            {
                                                "name": "INFLUXDB_HOST",
                                                "value": "localhost"
                                            },
                                            {
                                                "name": "INFLUXDB_PORT",
                                                "value": "8086"
                                            },
                                            {
                                                "name": "INFLUXDB_USER",
                                                "value": "root"
                                            },
                                            {
                                                "name": "INFLUXDB_PASS",
                                                "value": "hyperpilot"
                                            },
                                            {
                                                "name": "GF_INSTALL_PLUGINS",
                                                "value": "grafana-clock-panel,grafana-piechart-panel"
                                            }
                                        ],
                                        "image": "grafana\/grafana",
                                        "name": "grafana",
                                        "args": [
                                            "--repo=https:\/\/107.178.222.220\/api\/plugins"
                                        ],
                                        "ports": [
                                            {
                                                "containerPort": 3000,
                                                "hostPort": 3000,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "500.0m",
                                                "memory": "300Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "influxsrv"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "pathfinder"
                        },
                        "name": "pathfinder",
                        "namespace": "default"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "pathfinder"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "pathfinder"
                                }
                            },
                            "spec": {
                                "hostname": "pathfinder",
                                "containers": [
                                    {
                                        "image": "hyperpilot\/pathfinder",
                                        "name": "pathfinder",
                                        "env": [
                                            {
                                                "value": "http:\/\/influxsrv:8086",
                                                "name": "INFLUXDB_URL"
                                            },
                                            {
                                                "value": "root",
                                                "name": "INFLUXDB_USERNAME"
                                            },
                                            {
                                                "value": "hyperpilot",
                                                "name": "INFLUXDB_PASSWORD"
                                            }
                                        ],
                                        "ports": [
                                            {
                                                "containerPort": 8080,
                                                "hostPort": 7878,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "512.0m",
                                                "memory": "100Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "pathfinder"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "goddd"
                        },
                        "name": "goddd",
                        "namespace": "default"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "goddd"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "goddd"
                                }
                            },
                            "spec": {
                                "hostname": "goddd",
                                "containers": [
                                    {
                                        "image": "hyperpilot\/goddd",
                                        "name": "goddd",
                                        "env": [
                                                                                {
                                "value": "http:\/\/pathfinder:8080",
                                "name": "ROUTINGSERVICE_URL"
                            },
                            {
                                "value": "http:\/\/influxsrv:8086",
                                "name": "INFLUXDB_URL"
                            },
                            {
                                "value": "root",
                                "name": "INFLUXDB_USERNAME"
                            },
                            {
                                "value": "hyperpilot",
                                "name": "INFLUXDB_PASSWORD"
                            }
                                        ],
                                        "args": [
                                            "\/goddd",
                                            "-inmem"
                                        ]
                                        "ports": [
                                            {
                                                "containerPort": 8080,
                                                "hostPort": 3000,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "512.0m",
                                                "memory": "100Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "goddd"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "locust-master"
                        },
                        "name": "locust-master",
                        "namespace": "default"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "locust-master"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "locust-master"
                                }
                            },
                            "spec": {
                                "hostname": "locust-master",
                                                              "volumes": [
                                                                                                       {
                                                "hostPath": {
                                                  "path": "\/locust_file",
                                                "name": "locust_file"
                                            }

                                    {
                                        "hostPath": {
                                            "path": "\/"
                                        },
                                        "name": "root"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/cgroup"
                                        },
                                        "name": "cgroup"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/sys"
                                        },
                                        "name": "sys"
                                    }
   ],
                                        "volumeMounts": [
                                        ],
                                "containers": [
                                    {
                                        "image": "hyperpilot\/locust",
                                        "name": "locust-master",
                                        "args": [
                                           "--master",
                                           "--host=http:\/\/goddd:8080",
                                          "--consumer",
                                          "--consumer-influx-endpoint=influxsrv:8086",
                                          "--consumer-influx-db=locust_results",
                                          "-f",
                                          "\/locust_file\/locustfile.py"
                                        ]
                                        "ports": [
                                            {
                                                "containerPort": 8089,
                                                "hostPort": 8089,
                                                "protocol": "TCP"
                                            },
                                            {
                                                "containerPort": 5557,
                                                "hostPort": 5557,
                                                "protocol": "TCP"
                                            },
                                            {
                                                "containerPort": 5558,
                                                "hostPort": 5558,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "128.0m",
                                                "memory": "150Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "locust-master"
            },
            {
                "volumes": [
                    {
                        "host": {
                            "sourcePath": "\/home\/ec2-user\/locust_file\/"
                        },
                        "name": "locust_file"
                    }
                ],
                "containerDefinitions": [
                    {
                        "mountPoints": [
                            {
                                "containerPath": "\/locust_file",
                                "sourceVolume": "locust_file"
                            }
                        ],
                        "essential": true,
                        "portMappings": [
                            {
                                "hostPort": 8089,
                                "containerPort": 8089
                            },
                            {
                                "hostPort": 5557,
                                "containerPort": 5557
                            },
                            {
                                "hostPort": 5558,
                                "containerPort": 5558
                            }
                        ],
                        "command": [
                            "--master",
                            "--host=http:\/\/goddd:8080",
                            "--consumer",
                            "--consumer-influx-endpoint=influxsrv:8086",
                            "--consumer-influx-db=locust_results",
                            "-f",
                            "\/locust_file\/locustfile.py"
                        ],
                        "memory": 150,
                        "cpu": 128,
                        "image": "hyperpilot\/locust",
                        "name": "locust-master"
                    }
                ],
                "family": "locust-master"
            },
            {
                "volumes": [
                    {
                        "host": {
                            "sourcePath": "\/home\/ec2-user\/locust_file\/"
                        },
                        "name": "locust_file"
                    }
                ],
                "containerDefinitions": [
                    {
                        "mountPoints": [
                            {
                                "containerPath": "\/locust_file",
                                "sourceVolume": "locust_file"
                            }
                        ],
                        "essential": true,
                        "ulimits": [
                            {
                                "name": "nofile",
                                "hardLimit": 12000,
                                "softLimit": 12000
                            }
                        ],
                        "memory": 300,
                        "cpu": 500,
                        "command": [
                            "--slave",
                            "--master-host=locust-master",
                            "-H",
                            "http:\/\/goddd:8080",
                            "-f",
                            "\/locust_file\/locustfile.py"
                        ],
                        "image": "hyperpilot\/locust",
                        "name": "locust-slave"
                    }
                ],
                "family": "locust-slave"
            }
        ]
    }
}
