                {
                    "mountPoints": [{
                        "containerPath": "\/var\/run\/docker.sock",
                        "sourceVolume": "docker_sock"
                    }],
                    "portMappings": [{
                        "hostPort": 7778,
                        "containerPort": 7778
                    }],
                    "essential": true,
                    "memory": 50,
                    "cpu": 128,
                    "image": "hyperpilot\/benchmark-agent",
                    "name": "benchmark-agent"
                }
            ],
            "family": "dddelivery-angularjs"
        },
        {
            "volumes": [],
            "containerDefinitions": [{
                "environment": [{
                    "value": "http:\/\/influxsrv:8086",
                    "name": "INFLUXDB_URL"
                }, {
                    "value": "root",
                    "name": "INFLUXDB_USERNAME"
                }, {
                    "value": "hyperpilot",
                    "name": "INFLUXDB_PASSWORD"
                }],
                "portMappings": [{
                    "hostPort": 7878,
                    "containerPort": 8080
                }],
                "essential": true,
                "memory": 100,
                "cpu": 512,
                "image": "hyperpilot\/pathfinder",
                "name": "pathfinder"
            }],
            "family": "pathfinder"
        },
        {
            "volumes": [],
            "containerDefinitions": [{
                "environment": [{
                    "value": "http:\/\/pathfinder:8080",
                    "name": "ROUTINGSERVICE_URL"
                }, {
                    "value": "http:\/\/influxsrv:8086",
                    "name": "INFLUXDB_URL"
                }, {
                    "value": "root",
                    "name": "INFLUXDB_USERNAME"
                }, {
                    "value": "hyperpilot",
                    "name": "INFLUXDB_PASSWORD"
                }],
                "portMappings": [{
                    "hostPort": 3000,
                    "containerPort": 8080
                }],
                "command": [
                    "\/goddd",
                    "-inmem"
                ],
                "essential": true,
                "memory": 100,
                "cpu": 512,
                "image": "hyperpilot\/goddd",
                "name": "goddd"
            }],
            "family": "goddd"
        },
        {
            "volumes": [{
                "host": {
                    "sourcePath": "\/home\/ec2-user\/locust_file\/"
                },
                "name": "locust_file"
            }],
            "containerDefinitions": [{
                "mountPoints": [{
                    "containerPath": "\/locust_file",
                    "sourceVolume": "locust_file"
