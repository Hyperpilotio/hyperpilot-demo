                    {
                        "hostPort": 5557,
                        "containerPort": 5557
                    },
                    {
                        "hostPort": 5558,
                        "containerPort": 5558
                    }
                ],
                "command": [
                    "--master",
                    "--host=http:\/\/goddd:8080",
                    "--consumer",
                    "--consumer-influx-endpoint=influxsrv:8086",
                    "--consumer-influx-db=locust_results",
                    "-f",
                    "\/locust_file\/locustfile.py"
                ],
                "memory": 150,
                "cpu": 128,
                "image": "hyperpilot\/locust",
                "name": "locust-master"
            }],
            "family": "locust-master"
        },
        {
            "volumes": [{
                "host": {
                    "sourcePath": "\/home\/ec2-user\/locust_file\/"
                },
                "name": "locust_file"
            }],
            "containerDefinitions": [{
                "mountPoints": [{
                    "containerPath": "\/locust_file",
                    "sourceVolume": "locust_file"
                }],
                "essential": true,
                "ulimits": [{
                    "name": "nofile",
                    "hardLimit": 12000,
                    "softLimit": 12000
                }],
                "memory": 300,
                "cpu": 500,
                "command": [
                    "--slave",
                    "--master-host=locust-master",
                    "-H",
                    "http:\/\/goddd:8080",
                    "-f",
                    "\/locust_file\/locustfile.py"
                ],
                "image": "hyperpilot\/locust",
                "name": "locust-slave"
            }],
            "family": "locust-slave"
        }
    ]
