{
    "name": "in-cluster",
    "region": "us-east-1",
    "nodeMapping": [
        {
            "task": "deployer",
            "id": 1
        },
        {
            "task": "workload-profiler",
            "id": 1
        },
        {
            "task": "analyzer",
            "id": 2
        },
        {
            "task": "analyzer-ui",
            "id": 2
        },
        {
            "task": "mongo-serve",
            "id": 1
        }
    ],
    "clusterDefinition": {
        "nodes": [
            {
                "instanceType": "t2.large",
                "id": 1
            },
            {
                "instanceType": "t2.large",
                "id": 2
            }
        ]
    },
    "kubernetes": {
        "secrets": [
            {
                "apiVersion": "v1",
                "kind": "Secret",
                "metadata": {
                    "name": "myregistrykey"
                },
                "type": "kubernetes.io\/dockerconfigjson",
                "data": {
                    ".dockerconfigjson": "ewoJImF1dGhzIjogewoJCSJodHRwczovL2luZGV4LmRvY2tlci5pby92MS8iOiB7CgkJCSJhdXRoIjogImFIbHdaWEp3YVd4dmRIVnpaWEk2YUhsd1pYSXhNak09IgoJCX0KCX0KfQ=="
                }
            }
        ],
        "taskDefinitions": [
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "deployer"
                        },
                        "name": "deployer"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "deployer"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "deployer"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "deployer",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/deployer:in-cluster",
                                        "name": "deployer",
                                        "ports": [
                                            {
                                                "containerPort": 7777,
                                                "hostPort": 7777,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "512.0m",
                                                "memory": "1024Mi"
                                            }
                                        },
                                        "env": [
                                            {
                                                "name": "AWS_ACCESS_KEY_ID",
                                                "value": ""
                                            },
                                            {
                                                "name": "AWS_SECRET_ACCESS_KEY",
                                                "value": ""
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "deployer"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "workload-profiler"
                        },
                        "name": "workload-profiler"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "workload-profiler"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "workload-profiler"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "workload-profiler",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/workload-profiler",
                                        "name": "deployer",
                                        "ports": [
                                            {
                                                "containerPort": 7779,
                                                "hostPort": 7779,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "512.0m",
                                                "memory": "2048Mi"
                                            }
                                        },
                                        "env": [
                                            {
                                                "name": "AWS_ACCESS_KEY_ID",
                                                "value": ""
                                            },
                                            {
                                                "name": "AWS_SECRET_ACCESS_KEY",
                                                "value": ""
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "workload-profiler"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "mongo-serve"
                        },
                        "name": "mongo-serve"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "mongo-serve"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "mongo-serve"
                                }
                            },
                            "spec": {
                                "volumes": [
                                    {
                                        "hostPath": {
                                            "path": "\/home\/ubuntu\/mongo_data"
                                        },
                                        "name": "mongo-data"
                                    }
                                ],
                                "hostname": "mongo-serve",
                                "containers": [
                                    {
                                        "volumeMounts": [
                                            {
                                                "mountPath": "\/data\/db",
                                                "name": "mongo-data"
                                            }
                                        ],
                                        "image": "mongo:3.4",
                                        "command": [
                                            "mongod"
                                        ],
                                        "args": [
                                            "-auth"
                                        ],
                                        "name": "mongo-serve",
                                        "ports": [
                                            {
                                                "containerPort": 27017,
                                                "hostPort": 27017,
                                                "protocol": "TCP"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "mongo-serve"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "analyzer"
                        },
                        "name": "analyzer"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "analyzer"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "analyzer"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "analyzer",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/analyzer:tim",
                                        "name": "analyzer",
                                        "ports": [
                                            {
                                                "containerPort": 5000,
                                                "hostPort": 5000,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "1024.0m",
                                                "memory": "3072Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    0
                ],
                "family": "analyzer"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "analyzer-ui"
                        },
                        "name": "analyzer-ui"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "analyzer-ui"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "analyzer-ui"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "analyzer-ui",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/analyzer-ui",
                                        "name": "analyzer-ui",
                                        "ports": [
                                            {
                                                "containerPort": 3000,
                                                "hostPort": 3000,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "512.0m",
                                                "memory": "512Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "analyzer-ui"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "influxdb"
                        },
                        "name": "influxdb"
                    },
                    "spec": {
                        "replicas": 1,
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "influxdb"
                                }
                            },
                            "spec": {
                                "hostname": "influxdb",
                                "volumes": [
                                    {
                                        "hostPath": {
                                            "path": "\/home\/ubuntu\/influx_data"
                                        },
                                        "name": "influx-data"
                                    }
                                ],
                                "containers": [
                                    {
                                        "env": [
                                            {
                                                "name": "ADMIN_USER",
                                                "value": "root"
                                            },
                                            {
                                                "name": "PRE_CREATE_DB",
                                                "value": "snap"
                                            },
                                            {
                                                "name": "INFLUXDB_INIT_PWD",
                                                "value": "default"
                                            }
                                        ],
                                        "image": "hyperpilot\/influx:1.2.2.a",
                                        "name": "influxdb",
                                        "ports": [
                                            {
                                                "containerPort": 8083,
                                                "hostPort": 8083,
                                                "protocol": "TCP"
                                            },
                                            {
                                                "containerPort": 8086,
                                                "hostPort": 8086,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "volumeMounts": [
                                            {
                                                "mountPath": "\/var\/lib\/influxdb",
                                                "name": "influx-data"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "1024m",
                                                "memory": "4096Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "family": "influxdb",
                "portTypes": [
                    1,
                    1
                ]
            }
        ]
    }
}
