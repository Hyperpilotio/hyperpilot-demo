 {
    "name": "in-cluster",
    "region": "us-east-1",
    "nodeMapping": [
        {
            "task": "deployer",
            "id": 1
        },
        {
            "task": "workload-profiler",
            "id": 1
        },
        {
            "task": "analyzer",
            "id": 1
        },
        {
            "task": "mongo-serve",
            "id": 1
        }
    ],
    "clusterDefinition": {
        "nodes": [
            {
                "instanceType": "t2.medium",
                "id": 1
            }
        ]
    },
    "kubernetes": {
        "secrets": [
            {
                "apiVersion": "v1",
                "kind": "Secret",
                "metadata": {
                    "name": "myregistrykey"
                },
                "type": "kubernetes.io\/dockerconfigjson",
                "data": {
                    ".dockerconfigjson": "ewoJImF1dGhzIjogewoJCSJodHRwczovL2luZGV4LmRvY2tlci5pby92MS8iOiB7CgkJCSJhdXRoIjogImFIbHdaWEp3YVd4dmRIVnpaWEk2YUhsd1pYSXhNak09IgoJCX0KCX0KfQ=="
                }
            }
        ],
        "taskDefinitions": [
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "deployer"
                        },
                        "name": "deployer"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "deployer"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "deployer"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "deployer",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/deployer",
                                        "name": "deployer",
                                        "ports": [
                                            {
                                                "containerPort": 7777,
                                                "hostPort": 7777,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "512.0m",
                                                "memory": "1024Mi"
                                            }
                                        },
                                        "env": [
                                            {
                                                "name": "AWS_ACCESS_KEY_ID",
                                                "value": "AKIAIB2YMXXKLRAKQE6Q"
                                            },
                                            {
                                                "name": "AWS_SECRET_ACCESS_KEY",
                                                "value": "0Hlr6bdj2a3XF7YpEaPUTcGUckmY6Tq1mvj+\/TCL"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "deployer"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "workload-profiler"
                        },
                        "name": "workload-profiler"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "workload-profiler"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "workload-profiler"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "deployer",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/workload-profiler",
                                        "name": "deployer",
                                        "ports": [
                                            {
                                                "containerPort": 7779,
                                                "hostPort": 7779,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "limits": {
                                                "cpu": "512.0m",
                                                "memory": "1024Mi"
                                            }
                                        },
                                        "env": [
                                            {
                                                "name": "AWS_ACCESS_KEY_ID",
                                                "value": "AKIAIB2YMXXKLRAKQE6Q"
                                            },
                                            {
                                                "name": "AWS_SECRET_ACCESS_KEY",
                                                "value": "0Hlr6bdj2a3XF7YpEaPUTcGUckmY6Tq1mvj+\/TCL"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "workload-profiler"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "mongo-serve"
                        },
                        "name": "mongo-serve"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "mongo-serve"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "mongo-serve"
                                }
                            },
                            "spec": {
                                "volumes": [
                                    {
                                        "hostPath": {
                                            "path": "\/home\/ubuntu\/mongo_data"
                                        },
                                        "name": "mongo-data"
                                    }
                                ],
                                "hostname": "mongo-serve",
                                "containers": [
                                    {
                                        "volumeMounts": [
                                            {
                                                "mountPath": "\/data\/db",
                                                "name": "mongo-data"
                                            }
                                        ],
                                        "image": "mongo:3.0.15",
                                        "name": "mongo-serve",
                                        "ports": [
                                            {
                                                "containerPort": 27017,
                                                "hostPort": 27017,
                                                "protocol": "TCP"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    0
                ],
                "family": "mongo-serve"
            },
            {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "analyzer"
                        },
                        "name": "analyzer"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "analyzer"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "analyzer"
                                }
                            },
                            "spec": {
                                "imagePullSecrets": [
                                    {
                                        "name": "myregistrykey"
                                    }
                                ],
                                "hostname": "deployer",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "hyperpilot\/analyzer:tim",
                                        "name": "analyzer",
                                        "ports": [
                                            {
                                                "containerPort": 5050,
                                                "hostPort": 5050,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "512.0m",
                                                "memory": "1024Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    0
                ],
                "family": "analyzer"
            }
        ]
    }
}
