{
    "name": "mongo",
    "serviceNames": ["mongo-serve"],
    "loadTester": {
        "name": "benchmark-controller",
        "benchmarkController": {
            "command": {
                "image": "hyperpilot/load-tester:mongo",
                "parserUrl": "https://s3.amazonaws.com/hyperpilot-benchmark-parsers/mongo/parser.js",
                "path": "/mongo-perf/benchrun.py",
                "args": [
                    "-f",
                    "/mongo-perf/testcases/complex_update.js"
                ],
                "serviceConfigs": [
                    {
                        "name": "mongo-serve",
                        "hostConfig": {
                            "arg": "--host",
                            "description": ""
                        },
                        "portConfig": {
                            "arg": "--port",
                            "description": ""
                        }
                    }
                ],
                "intensityArgs": [
                    {
                        "name": "Number of connections",
                        "arg": "-t",
                        "startingValue": 1,
                        "step": 1
                    }
                ]
            }
        }
    },
    "type": "long-running",
    "slo": {
        "metric": "Update.SetWithMultiIndex.Random",
        "value": 500,
        "type": "throughput"
    }
}
