{
    "name": "mysql",
    "serviceNames": [
        "mysql-server"
    ],
    "loadTester": {
        "name": "tpcc-bench",
        "benchmarkController": {
            "initializeType": "stage",
            "initialize": {
                "path": "\/opt\/tpcc-mysql\/run.sh",
                "args": [
                    "mysql-server",
                    "tpcc",
                    "root",
                    "",
                    "3"
                ]
            },
            "command": {
                "path": "\/opt\/tpcc-mysql\/tpcc_start",
                "args": [
                    "-d",
                    "tpcc",
                    "-h",
                    "mysql-server",
                    "-u",
                    "root",
                    "-w",
                    "3",
                    "-r",
                    "3",
                    "-l",
                    "120",
                    "-i",
                    "120"
                ],
                "intensityArgs": [
                    {
                        "name": "Number of connections",
                        "arg": "-c",
                        "startingValue": 5,
                        "step": 10
                    }
                ]
            }
        }
    },
    "type": "long-running",
    "slo": {
        "metric": "95th_percentile",
        "value": 2.3,
        "type": "latency"
    }
}
