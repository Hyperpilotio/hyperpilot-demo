{
    "name": "redis",
    "serviceNames": [
        "redis-serve"
    ],
    "loadTester": {
        "name": "benchmark-controller",
        "benchmarkController": {
            "initialize": {
                "image": "redis:3-alpine",
                "path": "redis-cli",
                "args": [
                    "flushall"
                ],
                "serviceConfigs": [
                    {
                        "name": "redis-serve",
                        "hostConfig": {
                            "arg": "-h",
                            "description": "host of the service"
                        },
                        "portConfig": {
                            "arg": "-p",
                            "description": "port of the service"
                        }
                    }
                ]
            },
            "command": {
                "image": "redis:3-alpine",
                "path": "redis-benchmark",
                "args": [
                    "--csv",
                    "-n",
                    "10",
                    "-t",
                    "GET"
                ],
                "parserUrl": "https://s3.amazonaws.com/hyperpilot-benchmark-parsers/redis/parser.js",
                "serviceConfigs": [
                    {
                        "name": "redis-serve",
                        "hostConfig": {
                            "arg": "-h",
                            "description": "host of the service"
                        },
                        "portConfig": {
                            "arg": "-p",
                            "description": "port of the service"
                        }
                    }
                ],
                "intensityArgs": [
                    {
                        "name": "Number of connections",
                        "arg": "-c",
                        "startingValue": 5,
                        "step": 3
                    }
                ]
            }
        }
    },
    "type": "long-running",
    "slo": {
        "metric": "GET",
        "value": 57085,
        "type": "throughput"
    },
    "budget": {
        "value": 150,
        "unit": "dollar"
    },
    "deploymentTemplate": "analysis-base",
    "taskDefinitions": [
        {
            "nodeMapping": {
                "task": "redis-serve",
                "id": 2
            },
            "taskDefinition": {
                "deployment": {
                    "apiVersion": "extensions\/v1beta1",
                    "kind": "Deployment",
                    "metadata": {
                        "labels": {
                            "app": "redis-serve"
                        },
                        "name": "redis-serve"
                    },
                    "spec": {
                        "replicas": 1,
                        "selector": {
                            "matchLabels": {
                                "app": "redis-serve"
                            }
                        },
                        "template": {
                            "metadata": {
                                "labels": {
                                    "app": "redis-serve"
                                }
                            },
                            "spec": {
                                "hostname": "redis-serve",
                                "containers": [
                                    {
                                        "imagePullPolicy": "Always",
                                        "image": "redis:3.2.6",
                                        "name": "redis-serve",
                                        "ports": [
                                            {
                                                "containerPort": 6379,
                                                "hostPort": 6379,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {
                                            "requests": {
                                                "cpu": "500.0m",
                                                "memory": "100Mi"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "portTypes": [
                    1
                ],
                "family": "redis-serve"
            }
        }
    ]
}