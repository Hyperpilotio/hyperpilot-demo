{
    "name": "redis",
    "serviceNames": ["redis-serve"],
    "loadTester": {
        "name": "benchmark-controller",
        "benchmarkController": {
            "initialize": {
                "image": "redis:3-alpine",
                "path": "redis-cli",
                "args": [
                    "flushall"
                ],
                "serviceConfig": [
                    {
                        "name": "redis-serve",
                        "hostConfig": {
                            "arg": "-h",
                            "description": "host of the service"
                        },
                        "portConfig": {
                            "arg": "-p",
                            "description": "port of the service"
                        }
                    }
                ]
            },
            "command": {
                "image": "redis:3-alpine",
                "path": "redis-benchmark",
                "args": [
                    "--csv",
                    "-n",
                    "10",
                    "-t",
                    "GET"
                ],
                "parserUrl": "https://s3.amazonaws.com/hyperpilot-benchmark-parsers/redis/parser.js",
                "serviceConfig": [
                    {
                        "name": "redis-serve",
                        "hostConfig": {
                            "arg": "-h",
                            "description": "host of the service"
                        },
                        "portConfig": {
                            "arg": "-p",
                            "description": "port of the service"
                        }
                    }
                ],
                "intensityArgs": [
                    {
                        "name": "Number of connections",
                        "arg": "-c",
                        "startingValue": 5,
                        "step": 3
                    }
                ]
            }
        }
    },
    "type": "long-running",
    "slo": {
        "metric": "GET",
        "value": 57085,
        "type": "throughput"
    },
    "budget": {
        "value": 150,
        "unit": "dollar"
    }
}
