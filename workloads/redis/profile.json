{
    "applicationConfig": {
        "name": "redis",
        "loadTester": {
            "name": "redis-bench",
            "benchmarkController": {
                "command": {
                    "path": "\/usr\/bin\/redis-benchmark",
                    "args": [
                        "-h",
                        "redis-serve",
                        "-p",
                        "6379",
                        "--csv",
                        "-n",
                        "500000",
                        "-t",
                        "GET"
                    ],
                    "intensityArgs": [
                        {
                            "name": "Number of connections",
                            "arg": "-c",
                            "startingValue": 5,
                            "step": 3
                        }
                    ]
                }
            }
        },
        "type": "long-running",
        "slo": {
            "metric": "GET",
            "value": 35000,
            "type": "throughput"
        }
    },
    "stages": [
        {
            "benchmarks": [
                {
                    "name": "busycpu",
                    "count": 1,
                    "resources": {
                        "cpushares": 256
                    },
                    "image": "hyperpilot\/busycpu"
                }
            ]
        }
    ]
}
