{
    "deployment": "redis",
    "stages": [
        {
            "benchmarks": [
                {
                    "name": "busycpu",
                    "count": 1,
                    "resources": {
                        "cpushares": 256
                    },
                    "image": "hyperpilot\/busycpu"
                }
            ],
            "loadTest": {
                "loadController": {
                    "serviceName": "redis-bench",
                    "path": "\/usr\/bin\/redis-benchmark",
                    "args": [
                        "-h",
                        "redis-serve",
                        "-p",
                        "6379",
                        "--csv",
                        "-n",
                        "10000"
                    ]
                }
            }
        }
    ]
}
