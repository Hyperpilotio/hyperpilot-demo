                                    }
                                ],
                                "volumes": [
                                    {
                                        "hostPath": {
                                            "path": "\/sys\/fs\/cgroup"
                                        },
                                        "name": "cgroup"
                                    },
                                    {
                                        "hostPath": {
                                            "path": "\/var\/lib\/docker\/"
                                        },
                                        "name": "var-lib-docker"
