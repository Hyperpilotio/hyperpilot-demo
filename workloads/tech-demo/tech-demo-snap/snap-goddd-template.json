{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "5s"
    },
    "max-failures": -1,
    "workflow": {
        "collect": {
            "metrics": {
                "/hyperpilot/goddd/*": {}
            },
            "config": {
                "/hyperpilot/goddd": {
                    "endpoint": "http://goddd.default:8080"
                }
            },
            "process": [
                {
                    "plugin_name": "snap-plugin-processor-agent",
                    "config": {
                        "qos-data-store-url": "http://<%= QOS_DATA_STORE_SERVICE_HOST =>:7781",
                        "sla-goal": 150,
                        "metric-type": "quantile_90"
                    }
                }
            ],
            "publish": [
                {
                    "plugin_name": "influxdb",
                    "config": {
                        "host": "<%= INFLUXSRV_SERVICE_HOST =>",
                        "port": 8086,
                        "database": "snap",
                        "user": "root",
                        "password": "hyperpilot",
                        "https": false,
                        "skip-verify": false
                    }
                }
            ]
        }
    }
}