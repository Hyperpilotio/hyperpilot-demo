{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "5s"
    },
    "max-failures": -1,
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/use/compute/utilization": {
                },
                "/intel/use/compute/saturation": {
                },
                "/intel/use/memory/utilization": {
                },
                "/intel/use/memory/saturation": {
                },
                "/intel/use/storage/sda/utilization": {
                },
                "/intel/use/storage/sda/saturation": {
                }
            },
            "config": {
                "/intel/use": {
                    "proc_path": "\/proc_host"
                }
            },
            "tags": {
                "/intel": {
                    "nodename": "<%= NODE_NAME =>",
                    "deploymentId": "<%= DEPLOYMENT_ID =>"
                }
            },
            "publish": [
                {
                    "plugin_name": "influxdb",
                    "config": {
                        "host": "<%= INFLUXSRV_SERVICE_HOST =>",
                        "port": 8086,
                        "database": "snapaverage",
                        "user": "root",
                        "password": "default",
                        "https": false,
                        "skip-verify": false
                    }
                }
            ]
        }
    }
}
